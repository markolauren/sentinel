    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "let TotalIngestion=toscalar(\r\nUsage\r\n| where DataType == tolower(DataType)\r\n| where TimeGenerated {TimeRange:query}\r\n| summarize IngestionVolume=sum(Quantity));\r\nUsage\r\n| where DataType == tolower(DataType)\r\n| where TimeGenerated {TimeRange:query}\r\n| project DataType, Solution, Quantity, IsBillable\r\n| summarize IngestionVolume=sum(Quantity), IngestionPercent=(sum(Quantity))/TotalIngestion by DataType\r\n//| sort by DataType asc\r\n| sort by IngestionVolume desc",
        "size": 0,
        "noDataMessage": "Select a table from the above list",
        "timeContextFromParameter": "TimeRange",
